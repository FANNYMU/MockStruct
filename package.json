    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/node": "^24.0.14",
    "commander": "^14.0.0",
    "ts-morph": "^26.0.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "3.2.4",
    "vitest": "^3.2.4"
  }
}
